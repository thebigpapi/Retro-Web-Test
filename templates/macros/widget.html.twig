{% if type == "driver-results" %}
    <div class="list-entry centered head">
        <div>Name</div>
        <div>Version</div>
        <div>Size</div>
        <div>File name</div>
		<div>Release date</div>
    </div>
    {% for driver in entity %}
        <div class="list-entry centered bios {% if loop.index is divisible by(2) %}alt{% endif %}">
            <div class="header-label">Name</div>
            <div><a class="result-link" href="{{ path("driver_show", {id: driver.getId()}) }}">{{ driver.getName() }}</a></div>
            <div class="header-label">Version</div>
            <div>{{ driver.getFileVersion() ? driver.getFileVersion() : 'Unknown' }}</div>
            <div class="header-label">Size</div>
            <div>{{driver.getSize() ? driver.getSizeFormatted() : "<i>unknown</i>" }}</div>
            <div class="header-label">File name</div>
            <div><a class="combo" href="{{ vich_uploader_asset(driver, 'file')}}">
				<img src="{{ absolute_url(asset('build/icons/dw.svg')) }}" alt="" width="16" style="vertical-align: middle;">{{ driver.getFileNameSimple() }}</a>
			</div>
			<div class="header-label">Last edited</div>
			<div>
				<i>{{ driver.getReleaseDateString() }}</i>
			</div>
        </div>
    {% endfor %}
{% endif %}
{% if type == "chipset-results" %}
    <div class="list-entry centered head">
		<div>Name</div>
		<div>Parts</div>
		<div>Aliases</div>
        <div>Release date</div>
	</div>
	{% for chipset in entity %}
	<div class="list-entry centered bios {% if loop.index is divisible by(2) %}alt{% endif %}{% if chipset.getExpansionChips() is empty %} nows{% endif %}">
		<div class="header-label">Name</div>
		<div><a class="result-link" href="{{ path("chipset_show", {id: chipset.getId()}) }}">{{ chipset.getFullName() }}</a></div>
		<div class="header-label">Parts</div>
		<div>{% for chipPart in chipset.getExpansionChips() %}
			<span class="text-block" style="margin-right: 5px;">{{ chipPart.getFullName() }}</span>
		{% endfor %}</div>
		<div class="header-label">Aliases</div>
		<div>{% if chipset.getChipsetAliases() is not empty %}
			{% for alias in chipset.getChipsetAliases() %}
				<span>{{ alias.getFullAliasName() }}{% if not loop.last %} |{% endif %}</span>
			{% endfor %}
		{% endif %}</div>
        <div class="header-label">Release date</div>
		<div>{{ chipset.getReleaseDate() ? chipset.getReleaseDateString() : 'Unknown' }}</div>
	</div>
	{% endfor %}
{% endif %}
{% if type == "expansionchip-results" %}
    <div class="list-entry centered head">
		<div>Name</div>
		<div>Type</div>
		<div>Aliases</div>
        <div>Device ID</div>
	</div>
	{% for chip in entity %}
	<div class="list-entry centered bios {% if loop.index is divisible by(2) %}alt{% endif %}">
		<div class="header-label">Name</div>
		<div><a class="result-link" href="{{ path("expansion_chip_show", {id: chip.getId()}) }}">{{ chip.getFullName() }}</a></div>
		<div class="header-label">Type</div>
		<div>{{ chip.getType() }}</div>
		<div class="header-label">Aliases</div>
		<div class="tag-container">{% if chip.getChipAliases() is not empty %}
			{% for alias in chip.getChipAliases() %}
				<span class="text-block">{{ alias.getFullAliasName() }}</span>
			{% endfor %}
		{% endif %}</div>
        <div class="header-label">Device ID</div>
		<div>{% if chip.getPciDevs() is not empty %}
			{% set len = chip.getPciDevs()|length %}
			{% for dev in chip.getPciDevs()|slice(0,6) %}
				{{ dev.getDev() }}{% if not loop.last %}, {% else %}{% if len > 6 %}, ...{% endif %}{% endif %}
			{% endfor %}
		{% endif %}</div>
	</div>
	{% endfor %}
{% endif %}
