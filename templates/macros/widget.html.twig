{% if type == "driver-results" %}
    <div class="list-entry centered head">
        <div>Name</div>
        <div>Version</div>
        <div>Last edited</div>
        <div>Media type</div>
        <div>Size</div>
        <div>File name</div>
    </div>
    {% for driver in entity %}
        <div class="list-entry centered bios {% if loop.index is divisible by(2) %}alt{% endif %}">
            <div class="header-label">Name</div>
            <div><a class="result-link" href="{{ path("driver_show", {id: driver.getId()}) }}">{{ driver.getName() }}</a></div>
            <div class="header-label">Version</div>
            <div>{{ driver.getFileVersion() ? driver.getFileVersion() : 'Unknown' }}</div>
            <div class="header-label">Last edited</div>
            <div id="index-datetime"><div class="div-datetime" style="padding:0;"><span class="perk-date">{{ driver.getLastEdited()|date("Y-m-d\\TH:i:s", "GMT") }}Z</span></div></div>
            <div class="header-label">Media type</div>
            <div style="display: flex;align-items: center;">{% for relation in driver.getMediaTypeFlags() %}
                <i style="vertical-align:super;">{{relation.getCount()}}x</i>
				<img src="{{ vich_uploader_asset(relation.getMediaTypeFlag(), 'icon')}}" style="padding-left: 5px;" alt="{{relation.getMediaTypeFlag().getName()}}" title="{{relation.getMediaTypeFlag().getName()}}" width="32">{% if not loop.last %}, {% endif %}
			{% endfor %}</div>
            <div class="header-label">Size</div>
            <div>
            {% if driver.getSize() %}
					{% if driver.getSize() > 1048575 %}{{ (driver.getSize() / 1048576)|number_format(2) }}&nbsp;MB
					{% elseif driver.getSize() > 1023 %}{{ (driver.getSize() / 1024)|number_format(2)}}&nbsp;KB
					{% else %}{{ driver.getSize()}}&nbsp;Bytes{% endif %}
			{% endif %}
            </div>
            <div class="header-label">File name</div>
            <div><a class="combo" href="{{ vich_uploader_asset(driver, 'file')}}">
				<img src="{{ absolute_url(asset('build/icons/dw.svg')) }}" alt="" width="16" style="vertical-align: middle;">{% set fname = driver.getFileName()|slice(0,driver.getFileName()|length - 27) ~ driver.getFileName()[driver.getFileName()|length - 4 :] %}{{ fname }}</a>
			</div>
        </div>
    {% endfor %}
{% endif %}
{% if type == "chipset-results" %}
    <div class="list-entry centered head">
		<div>Name</div>
		<div>Parts</div>
		<div>Aliases</div>
        <div>Release date</div>
	</div>
	{% for chipset in entity %}
	<div class="list-entry centered bios {% if loop.index is divisible by(2) %}alt{% endif %}{% if chipset.getExpansionChips() is empty %} nows{% endif %}">
		<div class="header-label">Name</div>
		<div><a class="result-link" href="{{ path("chipset_show", {id: chipset.getId()}) }}">{{ chipset.getNameWithoutParts() }}</a></div>
		<div class="header-label">Parts</div>
		<div>{% for chipPart in chipset.getExpansionChips() %}
			<span class="text-block" style="margin-right: 5px;">{{ chipPart.getNameWithManufacturer() }}</span>
		{% endfor %}</div>
		<div class="header-label">Aliases</div>
		<div>{% if chipset.getChipsetAliases() is not empty %}
			{% for alias in chipset.getChipsetAliases() %}
				<span>{{ alias.getFullAliasName() }}{% if not loop.last %} |{% endif %}</span>
			{% endfor %}
		{% endif %}</div>
        <div class="header-label">Release date</div>
		<div>{{ chipset.getReleaseDate() ? chipset.getReleaseDateString() : 'Unknown' }}</div>
	</div>
	{% endfor %}
{% endif %}
{% if type == "expansionchip-results" %}
    <div class="list-entry centered head">
		<div>Name</div>
		<div>Type</div>
		<div>Aliases</div>
        <div>Device ID</div>
	</div>
	{% for chip in entity %}
	<div class="list-entry centered bios {% if loop.index is divisible by(2) %}alt{% endif %}">
		<div class="header-label">Name</div>
		<div><a class="result-link" href="{{ path("expansion_chip_show", {id: chip.getId()}) }}">{{ chip.getNameWithManufacturer() }}</a></div>
		<div class="header-label">Type</div>
		<div>{{ chip.getType() }}</div>
		<div class="header-label">Aliases</div>
		<div class="tag-container">{% if chip.getChipAliases() is not empty %}
			{% for alias in chip.getChipAliases() %}
				<span class="text-block">{{ alias.getFullAliasName() }}</span>
			{% endfor %}
		{% endif %}</div>
        <div class="header-label">Device ID</div>
		<div>{% if chip.getPciDevs() is not empty %}
			{% set len = chip.getPciDevs()|length %}
			{% for dev in chip.getPciDevs()|slice(0,6) %}
				{{ dev.getDev() }}{% if not loop.last %}, {% else %}{% if len > 6 %}, ...{% endif %}{% endif %}
			{% endfor %}
		{% endif %}</div>
	</div>
	{% endfor %}
{% endif %}
