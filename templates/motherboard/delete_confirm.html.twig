{% extends '@EasyAdmin/page/content.html.twig' %}

{% block page_title 'Delete motherboard' %}
{% block page_content %}
<div class="col-3">
<div style="display: flex;flex-direction: column;align-items: center;">
	Successfully deleted motherboard with ID: {{id}}
	<a class="btn" href="{{ ea_url().setDashboard('App\\Controller\\Admin\\DashboardController').setController('App\\Controller\\Admin\\MotherboardCrudController').setAction('index').setEntityId('1')}}" style="display:inline-block;max-width:100px;">
		<img src="{{ absolute_url(asset('build/icons/tick.svg')) }}" alt="" width="24"> Continue</a>
</div>
{% endblock %}