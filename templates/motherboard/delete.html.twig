{% extends 'base.html.twig' %}

{% block title %}{% trans %}Delete{% endtrans %}{% endblock %}

{% block body %}
	<div class="title"><b>{% trans %}Delete{% endtrans %}</b></div>
	<p><b class="contrib">Are you sure you want to delete this motherboard ? ({{ motherboard.getManufacturerShortNameIfExist }} {{motherboard.getName}}) </b></p>
	{{ form_start(form, {'method': 'POST'}) }}
    <span>{{form_widget(form.Yes)}} {{form_widget(form.No)}}</span>
    {{ form_end (form)}}
{% endblock %}