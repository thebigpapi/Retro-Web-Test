{% extends 'admin/header.html.twig' %}

{% block title %}Manage CPU sockets{% endblock %}

{% block body %}
<div style="height:5em;"></div>
<h2 style="text-align:center;"><b>Edit:CPU Socket</b></h2>
{{ form_start(form, {'method': 'POST'}) }}
<div class="editasset">
	{{form_widget(form.save)}}
    <table class="editasset">
		<tr>
			<td>{{form_label(form.name)}}</td>
			<td>{{form_widget(form.name)}}</td>
		</tr>
        <tr>
			<td>{{form_label(form.type)}}</td>
			<td>{{form_widget(form.type)}}</td>
		</tr>
		<tr>
			<td>Platforms:</td>
			<td>{% include 'macros/entityList.html.twig' with {'entityName':'platforms', 'entityDisplayName': 'platform', 'entities':form.platforms} %}</td>
		</tr>
	</table> 
</div>
<div id="hiddenDiv" style="display: none;">
    {{ form_end(form) }}
</div>
<script src="{{ absolute_url(asset('assets/add-collection-widget.js')) }}"></script>
{% endblock %}