{% if objectList is not empty %}
    <table class="results">
        {% include 'admin/manage/memories/' ~ entityName ~ '_thead.html.twig' %}
        {% for object in objectList %}
            {% include 'admin/manage/memories/' ~ entityName ~ '.html.twig' with {'object':object} %}	
        {% endfor %}
    </table>
    {{ knp_pagination_render(objectList) }}
{% endif %}